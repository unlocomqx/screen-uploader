{
  "name": "screen-uploader",
  "version": "1.0.0",
  "description": "easily upload images from your clipboard",
  "main": "main.js",
  "scripts": {
    "build": "rollup -c",
    "dev-svelte": "rollup -c -w",
    "start": "sirv public --no-clear",
    "start-electron": "wait-on http://localhost:5000 && electron .",
    "dev": "run-p dev-svelte start-electron",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=public/assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . ScreenUploader --overwrite --asar=true --platform=win32 --arch=ia32 --icon=public/assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Screen Uploader\"",
    "package-linux": "electron-packager . ScreenUploader --overwrite --asar=true --platform=linux --arch=x64 --icon=public/assets/icons/png/icon.png --prune=true --out=release-builds",
    "package-all": "npm run package-mac && npm run package-win && npm run package-linux",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -mwl"
  },
  "author": "unlocomqx",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tsconfig/svelte": "^2.0.0",
    "concurrently": "^6.2.1",
    "electron": "^7.2.4",
    "electron-builder": "^22.9.1",
    "electron-packager": "^15.2.0",
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0",
    "wait-on": "^6.0.0"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "electron-clipboard-watcher": "^1.0.1",
    "electron-settings": "^3.2.0",
    "form-data": "^3.0.0",
    "idb-keyval": "^3.2.0",
    "material-icons": "^1.10.4",
    "play-sound": "^1.1.3",
    "sirv-cli": "^1.0.0",
    "svelte-local-storage-store": "^0.2.6"
  }
}
